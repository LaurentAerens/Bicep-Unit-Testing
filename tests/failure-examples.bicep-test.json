{
  "description": "Example showing how different assertion types report failures",
  "tests": [
    {
      "name": "This will pass - correct shouldBe",
      "input": "length([1, 2, 3])",
      "shouldBe": "3"
    },
    {
      "name": "This will fail - incorrect shouldBe",
      "input": "length([1, 2, 3])",
      "shouldBe": "5"
    },
    {
      "name": "This will fail - shouldNotBe matches actual",
      "input": "toUpper('test')",
      "shouldNotBe": "'TEST'"
    },
    {
      "name": "This will fail - shouldContain not found",
      "input": "concat('hello', 'world')",
      "shouldContain": "xyz"
    }
  ]
}
